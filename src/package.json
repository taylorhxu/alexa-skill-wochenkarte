{
  "name": "alexa-skill-mittagskarte",
  "version": "0.0.0",
  "dependencies": {
    "alexa-sdk": "*",
    "cheerio": "*",
    "request": "*",
    "string": "*",
    "@types/alexa-sdk": "*",
    "@types/cheerio": "*",
    "@types/node": "*",
    "@types/request": "*",
    "@types/string": "*"
  },
  "devDependencies": {
    "typescript": "*",
    "typings": "*"
  },
  "scripts": {
    "postinstall": "typings install && tsc",
    "build": "rm -rf build && tsc",
    "package": "mkdir -p target && cd build && zip -FSr \"../target/${npm_package_name}_${npm_package_version}.zip\" . && cd .. && zip -r \"target/${npm_package_name}_${npm_package_version}.zip\" node_modules",
    "clean": "rm -rf build node_modules target typings"
  }
}
